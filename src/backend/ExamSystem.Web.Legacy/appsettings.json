{
  "AppOptions": {
    "S3BucketName": "",
    "AllowedOriginsForCors": [
      "http://localhost:4200"
    ]
  },
  "AWS": {
    "Region": "",
    "Profile": ""
  },
  "AwsCredentialOptions": {
    "AccessKey": "",
    "SecretKey": ""
  },
  "GoogleRecaptchaOptions": {
    "SecretKey": "",
    "SiteKey": "",
    "Version": ""
  },
  "SmtpOptions": {
    "SenderName": "",
    "SenderEmail": "",
    "Host": "",
    "Username": "",
    "Password": "",
    "Port": 0,
    "UseSSL": true
  },
  "SerilogEmailSinkOptions": {
    "EmailFrom": "",
    "EmailTo": "",
    "EmailSubject": "",
    "SmtpHost": "",
    "SmtpPort": 0,
    "SmtpUsername": "",
    "SmtpPassword": "",
    "MinimumLogLevel": "Error"
  },
  "ConnectionStrings": {
    "ExamSystemDb": ""
  },
  "Serilog": {
    "Using": [
      "Serilog",
      "Serilog.Sinks.File",
      "Serilog.Sinks.MSSqlServer"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "ExamSystemDb",
          "sinkOptionsSection": {
            "tableName": "Logs"
          },
          "restrictedToMinimumLevel": "Error"
        }
      },
      {
        "Name": "File",
        "Args": {
          "restrictedToMinimumLevel": "Information",
          "path": "Logs/web-log-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
