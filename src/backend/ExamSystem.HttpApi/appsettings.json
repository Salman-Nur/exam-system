{
  "AppOptions": {
    "S3BucketName": "",
    "AllowedOriginsForCors": [
      "http://localhost:4200",
      "https://localhost:7008"
    ],
    "DataProtectionDirectoryPath": ""
  },
  "AWS": {
    "Region": "",
    "Profile": ""
  },
  "AwsCredentialOptions": {
    "AccessKey": "",
    "SecretKey": ""
  },
  "JwtOptions": {
    "Secret": "",
    "ExpiryMinutes": 0
  },
  "GoogleRecaptchaOptions": {
    "SecretKey": "",
    "SiteKey": "",
    "VerificationEndpoint": "https://www.google.com/recaptcha/api/siteverify"
  },
  "SmtpOptions": {
    "SenderName": "",
    "SenderEmail": "",
    "Host": "",
    "Username": "",
    "Password": "",
    "Port": 0,
    "UseSSL": true
  },
  "SerilogEmailSinkOptions": {
    "EmailFrom": "",
    "EmailTo": "",
    "EmailSubject": "",
    "SmtpHost": "",
    "SmtpPort": 0,
    "SmtpUsername": "",
    "SmtpPassword": "",
    "MinimumLogLevel": "Error"
  },
  "ConnectionStrings": {
    "ExamSystemDb": ""
  },
  "Serilog": {
    "Using": [
      "Serilog",
      "Serilog.Sinks.File",
      "Serilog.Sinks.MSSqlServer"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "ExamSystemDb",
          "sinkOptionsSection": {
            "tableName": "Logs"
          },
          "restrictedToMinimumLevel": "Error"
        }
      },
      {
        "Name": "File",
        "Args": {
          "restrictedToMinimumLevel": "Warning",
          "path": "Logs/web-log-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5065"
      }
    }
  },
  "AllowedHosts": "*"
}
